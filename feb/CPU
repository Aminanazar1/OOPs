package MODULE1;

public class cpu {
	double price;
	class Processor {
		double cores ;
		String manufacturer;
		double getCache ( ) {
			return 4.3 ;}}
	static class RAM{
		double memory;
		String manufacturer;
		double getClockSpeed() {
			return 5.5 ;}}
}
cpudetails.java
----
package MODULE1;

public class cpudetails {
	public static void main(String[] args) {
		cpu cpu = new cpu();
		cpu.Processor processor = cpu.new Processor();
		cpu.RAM ram =new cpu.RAM();
		System.out.println("Processor Cache =" + processor.getCache());
		System.out.println("RAM Clock speed =" + ram.getClockSpeed());
	}
}

output:
-----
Processor Cache =4.3
RAM Clock speed =5.5

